@{
    ViewData["Title"] = "Sumary";
    Layout = "~/Views/Shared/_Layoutpage.cshtml";
}
<link href="~/css/StyleSheet.css" rel="stylesheet" />
<link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
<div class="fixed is-flex is-flex-direction-column is-justify-content-center is-align-content-center is-align-items-center is-flex-wrap-wrap">
    <nav class="level">
        <div class="level-item has-text-centered">
            <div>
                <div class="buttons are-medium">
                    <a class="button " id="dashboardButton">
                        <i class="fas fa-tachometer-alt"></i>
                    </a>
                </div>
                <p class="sidebar-title">Dashboard</p>
            </div>
        </div>
    </nav>
    <nav class="level">
        <div class="level-item has-text-centered ">
            <div class="toEpisode">
                <div class="buttons are-medium" id="toEpisode">
                    <a class="button " id="toEpisodeButton">
                        <i class="fas fa-bars is-white"></i>
                    </a>
                </div>
                <p class="sidebar-title" id="toEpisodeTitle">Episodes</p>
            </div>
        </div>
    </nav>
</div>

<div class="container dashboard" style="margin-bottom:200px; margin-left:60px">
</div>

<script src="~/js/jquery-3.2.1.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        $.ajax({
            url: "@(Url.Action("getDashBoard", "Dashboard"))",
            dataType: 'html'

            , success: function (result) {
                $('.dashboard').html(result);
            }
            , error: function () {
                alert("something wrong");
            }
        });
        $("#dashboardButton").click(function () {
            $.ajax({
                cache: false,
                type: "GET",
                url: "@(Url.Action("getDashBoard", "Dashboard"))",
                success: function (data) {
                    $(".dashboard").html('');
                    $(".dashboard").html(data);

                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('Failed to retrieve books.');
                }
            });
        });
        $(".toEpisode").click(function () {
            $.ajax({
                cache: false,
                type: "GET",
                url: "@(Url.Action("getAll", "Novel"))",
                success: function (data) {
                    $(".dashboard").html('');
                    $(".dashboard").html(data);

                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('Failed to retrieve books.');
                }
            });
        });

    });
</script>